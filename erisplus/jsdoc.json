{
  "source": {
    "include": ["erisplus"],
    "includePattern": ".js$",
    "excludePattern": "(^|\\/|\\\\)erisplus(\\/|\\\\)docs(\\/|\\\\)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "name": "Eris Plus"
  },
  "opts": {
    "template": "node_modules/docdash",
    "recurse": false,
    "destination": "docs/erisplus",
    "readme": "erisplus/README.md"
  },
  "docdash": {
    "openGraph": {
      "title": "Eris Plus",
      "type": "website",
      "site_name": "Eris Plus"
    },
    "meta": {
      "title": "Eris Plus",
      "description": "Eris Plus",
      "keyword": "eris,discord,bot"
    },
    "menu": {
      "Eris": {
        "href": "https://github.com/abalabahaha/eris",
        "target": "_blank",
        "class": "menu-item",
        "id": "eris_link"
      },
      "Mei": {
        "href": "../api/index.html",
        "class": "menu-item",
        "id": "mei_link"
      }
    }
  }
}